// Half Adder - Subtractor

module HAS(D, D_N, Q, Q_N, Cin, Cout, O);
    input D, D_N, Q, Q_N, Cin;
    output Cout, O;

    wire D, D_N, Q, Q_N, Cin;
    wire Cout, O;

    // Internal wires declaration
    wire w1, w2;

    and(w1, D, Q_N, Cin);
    and(w2, D_N, Q, Cin);
    or(Cout, w1, w2);

    xor(O, Q, Cin);

endmodule